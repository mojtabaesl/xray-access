{
  "name": "xray-access",
  "version": "1.0.0",
  "author": "Mojtaba Eslami",
  "license": "MIT",
  "description": "Fraud detection by analyzing Xray access log",
  "bugs": {
    "url": "https://github.com/mojtabaesl/xray-access/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojtabaesl/xray-access.git"
  },
  "homepage": "https://github.com/mojtabaesl/xray-access#readme",
  "keywords": [
    "Xray",
    "access",
    "log",
    "Fraud",
    "ip",
    "limit"
  ],
  "type": "module",
  "main": "bin/index.js",
  "bin": {
    "xgray": "./bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "xgray": "ts-node-esm ./src/index.ts",
    "start": "npm run build && npm run global",
    "global": "npm i -g . && xgray",
    "build": "npm run clean:some && tsc --build",
    "clean:some": "tsc --build --clean",
    "clean:all": "rm -rf ./lib ./node_modules ./package-lock.json",
    "refresh": "npm run clean:all && npm install",
    "publish-package": "npm run build && npm publish",
    "prepare": "husky install",
    "upgrade": "npx npm-check -u"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.194",
    "@types/luxon": "3.3.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "cspell": "6.31.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "csv-parse": "5.3.8",
    "lodash": "4.17.21",
    "lowdb": "6.0.1",
    "luxon": "3.3.0"
  },
  "volta": {
    "node": "19.9.0",
    "npm": "9.6.3"
  }
}
